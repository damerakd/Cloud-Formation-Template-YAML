Resources:
  DB:
    Type: "AWS::RDS::DBInstance"
    Properties:
      AllocatedStorage: 10
      StorageType: gp2
      DBInstanceClass: db.t2.micro
      DBName: xxxxxx  # replace xxxxxx with databasename here
      Engine: MySQL
      MasterUsername: xxxxxx  # # replace xxxxxx with username
      MasterUserPassword: xxxxxx # replace xxxxxx with password
  S3:
    Type: "AWS::S3::Bucket"
    Properties:
      BucketName: xxxxxx # replace xxxxxx with random
